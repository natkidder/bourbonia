<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrallis.techconf.domain">
	<class name="PresentationTopic" table="presentationtopic" mutable="false">
	    <cache usage="read-only"/>
		<id
			column="PK_ID"
			name="Id"
			type="integer"
		>
			<generator class="identity" />
		</id>
		<property
			column="NAME"
			length="16"
			name="Name"
			not-null="true"
			type="string"
		 />
		<property
			column="DESCRIPTION"
			length="64"
			name="Description"
			not-null="false"
			type="string"
		 />
	</class>
</hibernate-mapping>
