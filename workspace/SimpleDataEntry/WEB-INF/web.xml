<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>AcqHibernateSpring</display-name>

	<servlet>
		<servlet-name>GetLabels</servlet-name>
		<servlet-class>web.GetLabels</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetLabels</servlet-name>
		<url-pattern>/GetLabels</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RefreshDatabase</servlet-name>
		<servlet-class>web.RefreshDatabase</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RefreshDatabase</servlet-name>
		<url-pattern>/RefreshDatabase</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetPerson</servlet-name>
		<servlet-class>web.GetPerson</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetPerson</servlet-name>
		<url-pattern>/GetPerson</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>InsertAcq</servlet-name>
		<servlet-class>web.InsertAcq</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InsertAcq</servlet-name>
		<url-pattern>/InsertAcq</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UpdateDao</servlet-name>
		<servlet-class>web.UpdateDao</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateDao</servlet-name>
		<url-pattern>/UpdateDao</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>DeleteDao</servlet-name>
		<servlet-class>web.DeleteDao</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteDao</servlet-name>
		<url-pattern>/DeleteDao</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RunBackup</servlet-name>
		<servlet-class>web.RunBackup</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RunBackup</servlet-name>
		<url-pattern>/RunBackup</url-pattern>
	</servlet-mapping>

</web-app>
