function chgImg() {
  document.secondImg.src="bills1.gif"
  }
var libraryOff
var libraryOn
if(document.images) {  // if browser supports the 'image' object
  libraryOff = new Image(72,22)
  libraryOn = new Image(72,22)
  libraryOff.src = "library_off.gif"
  libraryOn.src = "library_on.gif"
  }
</script>
